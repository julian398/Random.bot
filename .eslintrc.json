{
  "root": true,
  "rules": {
    "no-empty": ["warn", { "allowEmptyCatch": true }],
    "no-shadow-restricted-names": "off",
    "semi": "error",
    "no-debugger": "off",
    "no-prototype-builtins":"off"
  },
  "env": {
    "node": true
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended"
  ]
}